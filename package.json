{
  "name": "sami-maths-club",
  "version": "1.0.8",
  "author": "C-Squared Development",
  "homepage": "https://samicharity.co.uk",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "deploy:dev": "firebase use dev && firebase deploy",
    "android:prepare": "ng build app -c android && npx cap sync",
    "android:open": "npx cap open android",
    "android:run": "npm run android:prepare && npm run android:open",
    "pwa:build": "ng build app -c pwa",
    "lint": "ng lint",
    "test:unit": "ng test",
    "test:local": "concurrently \"ng serve\" \"cypress open\"",
    "test:ci": "run-p --race serve:build cypress:run",
    "cypress:run": "cypress run",
    "serve:build": "http-server ./www -a localhost -p 4200 -c-1",
    "generate:***TODO***": "switch to using server once sync'd",
    "generate:default-problems": "firestore-export --accountCredentials backups/dev.service.json --backupFile ./backups/defaultProblems.json",
    "backup:dev": "firestore-export --accountCredentials backups/dev.service.json --backupFile ./backups/dev.backup.json",
    "backup:prod": "firestore-export --accountCredentials backups/prod.service.json --backupFile ./backups/prod.backup.json",
    "restore:dev": "firestore-import --accountCredentials backups/dev.service.json --backupFile ./backups/dev.backup.json",
    "restore:prod": "firestore-import --accountCredentials backups/prod.service.json --backupFile ./backups/prod.backup.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.4",
